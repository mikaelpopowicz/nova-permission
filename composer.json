{
    "name": "mikaelpopowicz/nova-permission",
    "description": "Permission tool for Laravel Nova",
    "homepage": "https://github.com/mikaelpopowicz/nova-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "MikaÃ«l Popowicz",
            "email": "mikael.popowicz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mikaelpopowicz\\NovaPermission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mikaelpopowicz\\NovaPermission\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mikaelpopowicz\\NovaPermission\\PermissionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
